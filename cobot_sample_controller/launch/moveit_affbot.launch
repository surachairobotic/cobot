<launch>
  <arg name="debug" default="false" />
  <arg name="use_gui" default="false" />
  <include file="$(find affbot_urdf10)/launch/planning_context.launch">
    <arg name="load_robot_description" value="true"/>
  </include>
  <node pkg="tf" type="static_transform_publisher" name="virtual_joint_broadcaster_0" args="0 0 0 0 0 0 world base_link 100" />
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="true" output="screen" />
  <include file="$(find affbot_urdf10)/launch/move_group.launch">
    <arg name="allow_trajectory_execution" value="false"/>
    <arg name="info" value="true"/>
    <arg name="debug" value="$(arg debug)"/>
  </include>
</launch>
